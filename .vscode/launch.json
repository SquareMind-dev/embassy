{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Debug usart_idle",
            "cwd": "${workspaceFolder}",
            "connectUnderReset": true,
            "runtimeExecutable": "probe-rs-debugger",
            "runtimeArgs": [
                "debug"
            ],
            "chip": "STM32F446RETx",
            "flashingConfig": {
                "flashingEnabled": true,
                "resetAfterFlashing": true,
                "haltAfterReset": true
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "./target/thumbv7em-none-eabihf/debug/usart_idle",
                    "svdFile": "./examples/stm32f4/STM32F446.svd",
                    "rttEnabled": true
                }
            ]
        },
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Debug usart_circular_dma_idle",
            "cwd": "${workspaceFolder}",
            "connectUnderReset": true,
            "runtimeExecutable": "probe-rs-debugger",
            "runtimeArgs": [
                "debug"
            ],
            "chip": "STM32F446RETx",
            "flashingConfig": {
                "flashingEnabled": true,
                "resetAfterFlashing": true,
                "haltAfterReset": true
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "./target/thumbv7em-none-eabihf/debug/usart_circular_dma_idle",
                    "svdFile": "./examples/stm32f4/STM32F446.svd",
                    "rttEnabled": true
                }
            ]
        }
    ]
}